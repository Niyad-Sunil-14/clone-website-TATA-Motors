showSlides1(slideIndex);

function button(a) {
  showSlides1(slideIndex += a);
}

function dot(a) {
  showSlides1(slideIndex = a);
}

function showSlides1(a) {
  let j;
  let slides1 = document.getElementsByClassName("cards");
  let dots1 = document.getElementsByClassName("dot1");
  if (a > slides1.length) {slideIndex = 1}    
  if (a < 1) {slideIndex = slides1.length}
  for (j = 0; j < slides1.length; j++) {
    slides1[j].style.display = "none";  
  }
  for (j = 0; j < dots1.length; j++) {
    dots1[j].className = dots1[j].className.replace(" active", "");
  }
  slides1[slideIndex-1].style.display = "block";  
  dots1[slideIndex-1].className += " active";
} 












